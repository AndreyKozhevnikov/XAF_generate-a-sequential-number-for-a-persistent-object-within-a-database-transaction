<?xml version="1.0" encoding="utf-8"?>
<Application Title="GenerateUserFriendlyId" Logo="ExpressAppLogo">
  <NavigationItems StartupNavigationItem="Contact_ListView" />
  <SchemaModules>
    <SchemaModule Name="SystemModule" Version="17.2.8.0" IsNewNode="True" />
  </SchemaModules>
  <Views>
    <ListView Id="Address_ListView" IsGroupPanelVisible="True">
      <Columns>
        <ColumnInfo Id="FullAddress" SortIndex="-1" SortOrder="None" />
        <ColumnInfo Id="AddressId" SortOrder="Ascending" SortIndex="0" />
        <ColumnInfo Id="Province" SortOrder="Ascending" GroupIndex="0" />
      </Columns>
    </ListView>
    <ListView Id="Contact_ListView">
      <Columns>
        <ColumnInfo Id="FullName" SortIndex="-1" SortOrder="None" />
        <ColumnInfo Id="ContactId" SortOrder="Ascending" SortIndex="0" />
      </Columns>
    </ListView>
    <ListView Id="IDocument_ListView">
      <Columns>
        <ColumnInfo Id="Title" SortIndex="-1" SortOrder="None" />
        <ColumnInfo Id="DocumentId" SortIndex="0" SortOrder="Ascending" />
      </Columns>
    </ListView>
  </Views>
</Application>